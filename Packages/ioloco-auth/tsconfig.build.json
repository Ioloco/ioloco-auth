  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "declarationMap": false,
    "emitDeclarationOnly": true,
    "noEmit": false,
    "skipLibCheck": true,
    "incremental": false
  },
  "include": ["src"],
  "exclude": ["**/*.test.ts", "**/*.spec.ts", "**/*.stories.tsx"]
}
